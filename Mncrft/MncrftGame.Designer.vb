        ' Step 1: Determine Energy Cost for Mining and Woodcutting
        itemGSM("pickaxeCost", "set", GetEnergyCost("Pickaxe", 0))
        gominingbutton.Text = $"Go Mining ({itemGSM("pickaxeCost")})"

